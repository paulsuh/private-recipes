<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the current release version of Acorn and creates signed PKG.</string>
	<key>Identifier</key>
	<string>com.github.paul-suh-recipes.signing.Acorn</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Acorn</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.2.0</string>
	<key>ParentRecipe</key>
	<string>com.github.paul-suh-recipes.pkg.Acorn</string>
	<key>Process</key>
	<array>
       <dict>
        	<key>Processor</key>
        	<string>PkgSigner</string>
        	<key>Arguments</key>
        	<dict>
        		<key>signing_cert</key>
        		<string>Developer ID Installer: ps Enable, Inc. (CC4C75F33Y)</string>
        		<key>suffix_with_date</key>
        		<true/>
        	</dict>
        </dict>
	</array>
</dict>
</plist>
